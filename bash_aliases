alias upgrade='sudo -- sh -c "apt -y update && apt -y upgrade && apt -y clean && apt -y autoclean && apt -y autoremove"'
alias thmvpn='sudo openvpn --data-ciphers AES-256-CBC --config ~/Documents/vpn/seeu.ovpn'
alias htbvpn='sudo openvpn ~/Documents/vpn/lab_seeuinspace.ovpn'
alias aliases='cat ~/.bash_aliases'
alias nessus-start='sudo /bin/systemctl start nessusd.service && firefox http://localhost:8834'
alias nessus-stop='sudo /bin/systemctl stop nessusd.service'
alias return='cd ~ && clear'
alias cleanssh='rm ~/.ssh/known_hosts'
alias pythonserver='python3 -m http.server 80'
alias startftp='python -m pyftpdlib -w'
alias clearssh='rm ~/.ssh/known_hosts'
alias startssh='sudo systemctl start ssh && sudo ss -ntplu'
alias stopssh='sudo systemctl stop ssh && sudo ss -ntplu'
alias startapache='sudo systemctl start apache2 && sudo ss -ntplu && tail -f /var/log/apache2/access.log'
alias stopapache='sudo systemctl stop apache2 && sudo ss -ntplu'
alias cleariptablesrules='sudo iptables -P INPUT ACCEPT && sudo iptables -P FORWARD ACCEPT && sudo iptables -P OUTPUT ACCEPT && sudo iptables -F && sudo iptables -X'
alias neo4jstart='sudo neo4j start'
alias neo4jstop='sudo neo4j stop'
alias postgresql-start='sudo systemctl start postgresql && sudo systemctl status postgresql'
alias postgresql-stop='sudo systemctl stop postgresql && sudo systemctl status postgresql'
alias pipupgrade='python3 -m pip install --upgrade pip && python -m pip install --upgrade pip'
